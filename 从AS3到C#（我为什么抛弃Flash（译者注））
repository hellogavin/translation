<script>
function riatoggleRatelogCollapse(tarId, ctrlObj) {
if(document.getElementById(tarId).style.display == '') {
document.getElementById(tarId).style.display = 'none';
document.getElementById(tarId+'_table').style.display = 'none';
document.getElementById(tarId+'_collapse').style.width = 'auto';
document.getElementById(tarId+'_collapse').style.background = 'none';
document.getElementById(tarId+'_btn').style.background = 'none';
setcookie('ratecollapse', 1, 2592000);
ctrlObj.innerHTML = '展开';
document.getElementById(tarId+'_num').style.display = 'none';
} else {
document.getElementById(tarId).style.display = '';
document.getElementById(tarId+'_table').style.display = '';
document.getElementById(tarId+'_collapse').style.width = '85px';
document.getElementById(tarId+'_collapse').style.background = 'url(/x2img/ratecollapse.png) no-repeat';
document.getElementById(tarId+'_btn').style.background = 'url("../../static/image/common/arrow_top.gif") no-repeat scroll 100% 50% transparent';
setcookie('ratecollapse', 0, -2592000);
ctrlObj.innerHTML = '收起';
riaratemore(tarId, 1);
}
}
function riaratemore(tarId, type) {
var tagtr = document.getElementById(tarId+'_table').getElementsByTagName('tr');
var tagtrlen = tagtr.length;
for (var i=0; i<tagtrlen; i++) {
if (type==0 && i>4) {
tagtr[i].style.display = 'none';
} else {
tagtr[i].style.display = '';
}
}
if (document.getElementById(tarId+'_btn').innerHTML == '展开') {
document.getElementById(tarId+'_num').style.display = 'none';
document.getElementById(tarId+'_collapse').style.width = 'auto';
document.getElementById(tarId+'_collapse').style.background = 'none';
document.getElementById(tarId+'_btn').style.background = 'none';
}
if (type==1) {
document.getElementById(tarId+'_num').style.display = 'none';
}
}
</script>

<div class="riaratehead">
<div class="riaratecollapse" id="riaratelog_2283591_collapse"><a href="javascript:;" onclick="riatoggleRatelogCollapse('riaratelog_2283591', this);" class="y xi2 op" id="riaratelog_2283591_btn">收起</a></div>
