<input type="hidden" id="riamodthreadtype" value="">
<input type="hidden" id="riamodthreadvalue" value="0">
<script>
function riamodthreads(optgroup, operation, type) {
if (optgroup==1 && operation=='digest' && type==1) {
document.getElementById('riamodthreadtype').value = 'digest';
document.getElementById('riamodthreadvalue').value = '3';
}
if (optgroup==1 && operation=='digest' && type==0) {
document.getElementById('riamodthreadtype').value = 'digest';
document.getElementById('riamodthreadvalue').value = '0';
}
if (optgroup==1 && operation=='stick' && type==1) {
document.getElementById('riamodthreadtype').value = 'stick';
document.getElementById('riamodthreadvalue').value = '1';
}
if (optgroup==1 && operation=='stick' && type==0) {
document.getElementById('riamodthreadtype').value = 'stick';
document.getElementById('riamodthreadvalue').value = '0';
}

var operation = !operation ? '' : operation;
$('modactions').action = 'forum.php?mod=topicadmin&action=moderate&fid=' + fid + '&moderate[]=' + tid + '&handlekey=mods&infloat=yes&nopost=yes' + (optgroup != 3 && optgroup != 2 ? '&from=' + tid : '');
$('modactions').optgroup.value = optgroup;
$('modactions').operation.value = operation;

hideWindow('mods');
showWindow('mods', 'modactions', 'post', 0);
if(BROWSER.ie) {
doane(event);
}
}
</script>	</div>
